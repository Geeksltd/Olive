{
  "App.Resource.Version": "%APP_VERSION%",

  "ConnectionStrings": {
    "Default": "Database=Olive.Tests.Temp; Data Source=.\\SQLExpress; Integrated Security=SSPI; MultipleActiveResultSets=True;"
  },

  "Database": {
    "Providers": [
      {
        "AssemblyName": "Olive.Entities.Data.SqlServer.Tests.dll",
        "ProviderFactoryType": "AppData.AdoDotNetDataProviderFactory"
      }
    ],
    "StoragePath": "C:\\@Database.Files",
    "Profile": false,
    "Cache": {
      "Enabled": true,
      "ConcurrencyAware": true
    },
    "Transaction": {
      "Type": "Olive.Entities.Data.DbTransactionScope, Olive.Entities.Data",
      "DefaultIsolationLevel": "ReadCommitted",
      "EnforceForSave": true
    },
    "Audit": {
      "Insert": {
        "Action": false,
        "Data": false
      },
      "Update": {
        "Action": false,
        "Data": false
      },
      "Delete": {
        "Action": false,
        "Data": false
      }
    }
  }
}
